{
	"compilerOptions": {
		"target": "ES2017",
		"module": "commonjs",
		"strictNullChecks": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": false,
		"downlevelIteration": true,
		"forceConsistentCasingInFileNames": true
	}
}
