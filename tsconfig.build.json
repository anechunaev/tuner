{
	"compilerOptions": {
		"target": "ES2017",
		"module": "commonjs",
		"strictNullChecks": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"downlevelIteration": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"ES2019.Array"
		]
	}
}
